{
  "name": "iosAndroid",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ip": "ipconfig getifaddr en0",
    "renameapk": "run a query that renames builtapk to version",
    "buildapk": "cd android && ./gradlew assembleRelease",
    "buildrelease": "cd android && ./gradlew bundleRelease",
    "android": "react-native run-android",
    "pod-update": "cd ios && arch -x86_64 pod install --repo-update",
    "ios": "react-native run-ios",
    "b-ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios-build": "react-native bundle --entry-file ./index.js --platform ios --bundle-output ios/main.jsbundle --assets-dest ios",
    "i": "react-native bundle --minify --entry-file index.js --platform ios --dev false --bundle-output ./ios/main.jsbundle --assets-dest ./ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/netinfo": "^9.3.4",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/material-bottom-tabs": "^6.2.4",
    "@react-navigation/material-top-tabs": "^6.2.4",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@reduxjs/toolkit": "^1.8.6",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "lodash": "^4.17.21",
    "lottie-ios": "3.4.0",
    "lottie-react-native": "^5.1.4",
    "moment": "^2.29.4",
    "native-base": "^3.4.17",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-animatable": "^1.3.3",
    "react-native-countdown-component": "^2.7.1",
    "react-native-device-info": "^10.2.1",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-image-picker": "^4.10.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-onesignal": "^4.4.1",
    "react-native-paper": "^4.12.5",
    "react-native-reanimated": "^2.11.0",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "^3.18.0",
    "react-native-svg": "12.1.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tab-view": "^3.2.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.23.1",
    "react-redux": "^8.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
